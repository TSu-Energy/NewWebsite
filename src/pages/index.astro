import Stats2 from '~/components/widgets/Stats2.astro';
import type { loadAll } from 'js-yaml';
import HeroCarousel from '~/components/widgets/HeroCarousel.astro';

const metadata = {
  title: 'TSuEnergy — Electrochemistry Energy Storage Device Modelling and Management',
  ignoreTitleTemplate: true,
};

const stats = [
  { title: 'Downloads', amount: 132, unit: 'K', id: 'downloads-id' },
  { title: 'Stars', amount: 24, unit: 'K', id: 'starts-id' },
  { title: 'Forks', amount: 10, unit: 'K', id: 'forks-id' },
  { title: 'Users', amount: 48, unit: 'K', id: 'users-id' },
];
